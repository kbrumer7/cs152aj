<div class="container">
    <div class= "mapheader"> Click on the map to learn more about COVID-19 in that area. <br /> <a href="/contact"><u>Contact us</u></a></div>
    <script async src="https://cse.google.com/cse.js?cx=014315194782570216271:uhxbdq5zgck"></script>
    <div class="gcse-search"></div>
  <div class="row align-items-end">
      <h3 align="center">Current US statistics</h3>
      <table class="table table-bordered" align = "center">
        <tbody>
          <tr>
            <td>Positive Tests</td>
            <td><%=covidData['positive'].toLocaleString()%></td>
          </tr>
          <tr>
            <td>Negative Tests</td>
            <td><%=covidData['negative'].toLocaleString()%></td>
          </tr>
          <tr>
            <td>Total Tests</td>
            <td><%=covidData['total'].toLocaleString()%></td>
          </tr>
          <tr>
            <td>Total Deaths</td>
            <td><%=covidData['death'].toLocaleString()%></td>
          </tr>
        </tbody>
      </table>

      <h3 align="center" >Color Indicators of Reopening Phases</h3>
      <table class="table table-bordered" align = "center">
        <tbody>
          <tr>
            <td>Red</td>
            <td bgcolor="red">Phase 1</td>
          </tr>
          <tr>
            <td>Orange</td>
            <td  bgcolor="orange">Phase 2</td>
          </tr>
          <tr>
            <td>Blue</td>
            <td  bgcolor="blue">Phase 3</td>
          </tr>
          <tr>
            <td>Green</td>
            <td  bgcolor="green">Phase 4</td>
          </tr>
        </tbody>
      </table>
    </div>

